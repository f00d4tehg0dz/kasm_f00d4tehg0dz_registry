{
  "friendly_name": "Discord Screen Share Audio",
  "image_src": "blob:https://f00d4tehg0dz.github.io/e6b170db-82cc-44de-a9f7-4250ef22bc2b",
  "description": "Screenshare your streams directly to Discord without needing to use the same PC and tie up resources",
  "name": "f00d4tehg0dz/kasm-discord-screenshare:latest",
  "cores": 4,
  "memory": 4768,
  "gpu_count": 0,
  "cpu_allocation_method": "Inherit",
  "docker_registry": "https://index.docker.io/v1/",
  "categories": [
    "Multimedia"
  ],
  "run_config": {
    "hostname": "kasm",
    "privileged" : true
  },
  "require_gpu": false,
  "enabled": true,
  "image_type": "Container",
  "architecture": [
    "amd64"
  ],
  "exec_config": {
    "go": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --go --url \"$KASM_URL\"'"
    },
    "assign": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --assign --url \"$KASM_URL\"'"
    }
  }
}
